{
  "name": "ecr-login-token-action",
  "description": "GitHub Action to get ecr token for Docker containers hosted at Amazon ECR",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest --coverage",
    "pre-checkin": "yarn run format && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PhutureCorp/ecr-login-token-action.git"
  },
  "keywords": [
    "actions",
    "ecr",
    "login"
  ],
  "author": "Docker",
  "contributors": [
    {
      "name": "Adam Rogas"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/io": "^1.0.2",
    "aws-sdk": "^2.1274.0",
    "semver": "^7.3.4",
    "webpack-bundle-analyzer": "^4.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@vercel/ncc": "^0.23.0",
    "dotenv": "^8.2.0",
    "jest": "^26.1.0",
    "jest-circus": "^26.1.0",
    "jest-runtime": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5",
    "typescript-formatter": "^7.2.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  }
}
